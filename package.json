{
  "name": "bot-challenge",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/bot-challenge.js",
  "module": "./src/index.js",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "flow-bin": "^0.89.0",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "pixi.js": "^5.0.0-rc.2",
    "victor": "^1.1.0"
  },
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server --open",
    "server:run": "nodemon --exec babel-node --presets=@babel/preset-env,@babel/preset-flow -- server",
    "eslint": "eslint src/",
    "flow": "flow"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype",
      "jest"
    ],
    "extends": [
      "airbnb-base"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "src"
          ]
        }
      }
    },
    "env": {
      "jest/globals": true,
      "browser": true
    },
    "rules": {
      "object-curly-spacing": [
        "error",
        "never"
      ],
      "no-unused-expressions": [
        "error",
        {
          "allowShortCircuit": true
        }
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-flowtype": "^3.5.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "file-loader": "^3.0.1",
    "nodemon": "^1.18.9",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "license": "MIT"
}
